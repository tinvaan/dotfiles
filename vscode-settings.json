{
    "editor.rulers": [
        80,
        120
    ],
    "editor.mouseWheelZoom": true,
    "markdown.preview.fontSize": 13.5,
    "window.menuBarVisibility": "toggle",
    "editor.renderWhitespace": "boundary",
    "workbench.sideBar.location": "right",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.smoothScrolling": true,
    "breadcrumbs.enabled": true,
    "editor.fontLigatures": true,
    "editor.cursorStyle": "block",
    "editor.minimap.showSlider": "always",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "search.showLineNumbers": true,
    "editor.formatOnType": true,
    "window.zoomLevel": 0,
    "window.titleBarStyle": "native",
    "editor.cursorWidth": 2,
    "editor.suggestSelection": "first",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Sourcegraph Dark",
    "editor.fontFamily": "Dank Mono",
    "font-switcher.enableLivePreview": true,
    "font-switcher.fontFamily": "Andale Mono, Antikor Mono, Anonymous Pro, Anonymous Pro Minus, Code New Roman, Consolas, Courier, Courier Prime Code, Dank Mono, Droid Sans Mono, DejaVu Sans Code, DejaVu Sans Mono, Iosevka, Input Mono, Monaco, Monospace, Nitti, Noto Sans Mono, Operator Mono Book, Operator Mono Medium, Hasklig, JetBrains Mono, Fira Code Retina, Fira Mono, Cascadia Code, League Mono, Roboto Mono, Space Mono, Go Mono, San Francisco Mono, Source Code Pro, Source Code Variable, Ubuntu Mono",
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.linux": "/home/harish/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.helm-path.linux": "/home/harish/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.draft-path.linux": "/home/harish/.vs-kubernetes/tools/draft/linux-amd64/draft"
    },
    "editor.tokenColorCustomizations": {
        "[CodePen-Nights]": {
            "textMateRules": [
                {
                    "scope": "variable.parameter.function-call.python",
                    "settings": {
                        "foreground": "#E06C75"
                    }
                },
                {
                    "scope": "meta.function-call.arguments.python",
                    "settings": {
                        "foreground": "#D19A66"
                    }
                },
                {
                    "scope": [
                        "meta.function-call.python",
                        "meta.function-call.generic.python"
                    ],
                    "settings": {
                        "foreground": "#61AFEFFF"
                    }
                },
                {
                    "scope": [
                        "keyword.control.flow.python",
                        "comment.line.double-slash",
                        "comment.line.number-sign.python"
                    ],
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#9CA0B1"
                    }
                },
                {
                    "scope": [
                        "keyword.control.import.python",
                        "variable.language.special.self.python",
                        "variable.parameter.function.language.special.cls.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#FF8D41"
                    }
                }
            ]
        },
        "[CodeSandbox Black]": {
            "textMateRules": [
                {
                    "scope": "meta.function-call.arguments.python",
                    "settings": {
                        "foreground": "#D19A66",
                    }
                },
                {
                    "scope": [
                        "storage.type.class.python",
                        "storage.type.function.python"
                    ],
                    "settings": {
                        "foreground": "#977CDC"
                    }
                },
                {
                    "scope": [
                        "variable.language.special.self.python",
                        "variable.parameter.function-call.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#E06C75"
                    }
                },
                {
                    "scope": [
                        "support.type.python",
                        "meta.function-call.python",
                        "meta.function-call.generic.python",
                        "entity.name.function.decorator.python",
                        "punctuation.definition.decorator.python"
                    ],
                    "settings": {
                        "foreground": "#2794ee",
                    }
                },
                {
                    "scope": [
                        "comment.line.double-slash",
                        "comment.line.number-sign.python"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[GitHub Dark]": {
            "textMateRules": [
                {
                    "scope": [
                        "meta.member.access.python",
                        "entity.other.inherited-class.python"
                    ],
                    "settings": {
                        "foreground": "#FFAB70"
                    }
                },
                {
                    "scope": [
                        "variable.language.special.self.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#F97583"
                    }
                },
                {
                    "scope": [
                        "meta.member.access.python",
                        "meta.function-call.python",
                        "meta.function-call.generic.python",
                        "entity.name.function.decorator.python"
                    ],
                    "settings": {
                        "foreground": "#79B8FF"
                    }
                },
                {
                    "scope": "meta.function-call.arguments.python",
                    "settings": {
                        "foreground": "#FAFBFC"
                    }
                },
                {
                    "scope": [
                        "string.quoted",
                        "string.unquoted",
                        "string.quoted.single",
                        "string.quoted.double",
                        "string.quoted.triple",
                        "string.interpolated",
                        "string.regexp",
                        "string.other"
                    ],
                    "settings": {
                        "foreground": "#748ea8"
                    }
                },
            ]
        },
        "[GitHub Light]": {
            "textMateRules": [
                {
                    "scope": [
                        "meta.member.access.python",
                        "entity.other.inherited-class.python",
                        "entity.name.function.decorator.python"
                    ],
                    "settings": {
                        "foreground": "#ec7723",
                    }
                },
                {
                    "scope": [
                        "variable.language.special.self.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#D73A49"
                    }
                },
                {
                    "scope": [
                        "meta.member.access.python",
                        "meta.function-call.python",
                        "meta.function-call.generic.python",
                        "meta.function-call.arguments.python"
                    ],
                    "settings": {
                        "foreground": "#005CC5"
                    }
                },
                {
                    "scope": "meta.function-call.arguments.python",
                    "settings": {
                        "foreground": "#24292E"
                    }
                },
                {
                    "scope": [
                        "string.quoted",
                        "string.unquoted",
                        "string.quoted.single",
                        "string.quoted.double",
                        "string.quoted.triple",
                        "string.interpolated",
                        "string.regexp",
                        "string.other"
                    ],
                    "settings": {
                        "foreground": "#748ea8"
                    }
                },
            ]
        },
        "[Sourcegraph Dark]": {
            "textMateRules": [
                {
                    "scope": "meta.function-call.arguments",
                    "settings": {
                        // "foreground": "#72C3FC"
                        "foreground": "#cef7d6"
                    }
                },
                {
                    "scope": [
                        "meta.indexed-name",
                        "meta.function-call",
                    ],
                    "settings": {
                        "foreground": "#329AF0"
                    }
                },
                {
                    "scope": [
                        "variable.language.special.self.python",
                        "variable.parameter.function-call.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#FFA8A8"
                    }
                }
            ]
        },
        "[Night Owl (No Italics)]": {
            "textMateRules": [
                {
                    "scope": [
                        "meta.function-call",
                        "meta.function-call.generic.python",
                    ],
                    "settings": {
                        "foreground": "#7FDBCA",
                    }
                },
                {
                    "scope": [
                        "variable.language.special.self.python",
                        "variable.parameter.function-call.python",
                        "variable.parameter.function.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#FF5874"
                    }
                },
            ]
        }
    },
    "editor.fontSize": 15,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintPath": "/usr/bin/pylint",
    "python.linting.flake8Path": "/usr/bin/flake8"
}